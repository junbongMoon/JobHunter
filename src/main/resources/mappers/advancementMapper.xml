<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jobhunter.mapper.advancementmapper">

	<insert id="saveAdvancement" parameterType="com.jobhunter.dto.AdvancementDTO" useGeneratedKeys="true" 
	keyProperty="advancementNo" keyColumn="advancementNo">
	insert into advancement
	(title, writer, refUser, content)
	values
	(#{title}, #{writer}, #{refUser}, #{content})
	</insert>


</mapper>